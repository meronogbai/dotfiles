snippet script "script"
  async function main() {
    ${0}
  }

  main().catch((e) => {
    console.error(e)
    process.exitCode = 1
  })

snippet ctime "console.time ... console.timeEnd"
	console.time("${1:label}")
	${0:${VISUAL}}
	console.timeEnd("$1")

snippet sleep "sleep"
	await new Promise(res => setTimeout(res, ${0}))

snippet isentry "import sentry"
	import * as Sentry from "@sentry/${0}"
